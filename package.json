{
  "name": "pwa-2018",
  "version": "0.0.1",
  "author": "TJA",
  "description": "pwa-2018",
  "repository": {
    "url": "https://github.com/tja4472/ngrx-auth-module"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint || ECHO .",
    "e2e": "ng e2e",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
    "prettier:write": "prettier --write \"./{e2e,src}/**/*.{scss,ts}\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "6.1.8",
    "@angular/core": "6.1.8",
    "@angular/fire": "5.0.2",
    "@angular/forms": "6.1.8",
    "@angular/http": "6.1.8",
    "@angular/platform-browser": "6.1.8",
    "@angular/platform-browser-dynamic": "6.1.8",
    "@angular/pwa": "0.8.3",
    "@angular/router": "6.1.8",
    "@angular/service-worker": "6.1.8",
    "@ionic/angular": "4.0.0-beta.11",
    "@ngrx/effects": "6.1.0",
    "@ngrx/entity": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "core-js": "2.5.7",
    "firebase": "5.5.1",
    "rxjs": "6.3.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.2.3",
    "@angular/compiler": "6.1.8",
    "@angular/compiler-cli": "6.1.8",
    "@angular/language-service": "6.1.8",
    "@angular-devkit/build-angular": "0.8.3",
    "@ngrx/schematics": "6.1.0",
    "@types/node": "10.10.3",
    "codelyzer": "4.4.4",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "23.6.0",
    "jest-preset-angular": "6.0.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.14.3",
    "protractor": "5.4.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "2.9.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "moduleNameMapper": {
      "^@app/(.*)": "<rootDir>/src/app/$1",
      "^@ionic/core/loader": "<rootDir>/node_modules/@ionic/core/dist/esm/es5/ionic.define.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@ngrx|@ionic-native|@ionic)"
    ]
  }
}
